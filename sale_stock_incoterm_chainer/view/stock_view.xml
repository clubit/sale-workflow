<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <record id="delivery_incoterm" model="ir.ui.view">
            <field name="name">delivery.incoterm</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form"/>
